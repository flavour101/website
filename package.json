{
  "name": "com.flavour101.frontend",
  "version": "1.0.0",
  "description": "The Flavour 101 Frontend",
  "main": "public/index.html",
  "scripts": {
    "lint": "eslint src/* --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config webpack.dev.js",
    "start:prod": "webpack serve --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "docker:build": "docker build -t $npm_package_name:latest .",
    "docker:run": "docker run --name $npm_package_name -d -p 8080:80 $npm_package_name:latest",
    "docker": "npm run build && npm run docker:build && npm run docker:run",
    "docker:destroy": "docker stop $npm_package_name && docker rm $npm_package_name && docker rmi $npm_package_name"
  },
  "author": "Harmelodic",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-react": "^7.12.13",
    "@harmelodic/react-ui-lib": "^3.4.2",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.22.0",
    "marked": "^4.0.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "keywords": [
    "flavour101",
    "frontend",
    "react",
    "harmelodic",
    "redux"
  ],
  "license": "UNLICENSED"
}
