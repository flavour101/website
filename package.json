{
  "name": "com.flavour101.frontend",
  "version": "1.0.0",
  "description": "The Flavour 101 Frontend",
  "main": "public/index.html",
  "scripts": {
    "lint": "eslint src/* --fix",
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "webpack serve --config webpack.dev.js",
    "start:prod": "webpack serve --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "docker:build": "docker build -t $npm_package_name:latest .",
    "docker:run": "docker run --name $npm_package_name -d -p 8080:80 $npm_package_name:latest",
    "docker": "npm run build && npm run docker:build && npm run docker:run",
    "docker:destroy": "docker stop $npm_package_name && docker rm $npm_package_name && docker rmi $npm_package_name"
  },
  "author": "Harmelodic",
  "devDependencies": {
    "@babel/core": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@harmelodic/react-ui-lib": "3.4.6",
    "babel-loader": "8.2.5",
    "compression-webpack-plugin": "9.2.0",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-react": "7.29.4",
    "marked": "4.0.15",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "styled-components": "5.3.5",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0",
    "webpack-merge": "5.8.0"
  },
  "keywords": [
    "flavour101",
    "frontend",
    "react",
    "harmelodic",
    "redux"
  ],
  "license": "UNLICENSED"
}
